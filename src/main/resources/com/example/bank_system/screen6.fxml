         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <ComboBox fx:id="bank" layoutX="225.0" layoutY="150.0" onAction="#bank" prefWidth="150.0" promptText="Bank" />
      <ComboBox fx:id="employee" layoutX="225.0" layoutY="220.0" onAction="#employee" prefWidth="150.0" promptText="Employee" />

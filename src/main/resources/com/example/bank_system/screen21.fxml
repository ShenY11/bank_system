<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bank_system.Screen21Controller">
   <children>
      <Label layoutX="219.0" layoutY="48.0" text="Manage Users">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="createEmployeeButton" layoutX="221.0" layoutY="118.0" mnemonicParsing="false" onAction="#createEmployee" text="Create Employee Role">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
      <Button fx:id="createCustomerButton" layoutX="221.0" layoutY="167.0" mnemonicParsing="false" onAction="#createCustomer" text="Create Customer Role">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
      <Button fx:id="stopEmployeeButton" layoutX="227.0" layoutY="218.0" mnemonicParsing="false" onAction="#stopEmployee" text="Stop Employee Role">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
      <Button fx:id="stopCustomerButton" layoutX="227.0" layoutY="267.0" mnemonicParsing="false" onAction="#stopCustomer" text="Stop Customer Role">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="274.0" layoutY="325.0" mnemonicParsing="false" onAction="#goToAdminMenu" text="Back" />
   </children>
</AnchorPane>
